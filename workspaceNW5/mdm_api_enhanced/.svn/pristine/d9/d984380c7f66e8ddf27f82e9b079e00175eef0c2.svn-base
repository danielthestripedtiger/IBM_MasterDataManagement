/****** Script for SelectTopNRows command from SSMS  ******/
SELECT 'MDMSP'
      ,'ACTIVE_DIRECTORY'+[PersonID]
      ,[Full_Name]
      ,[First_Name]
      ,[Middle_Name]
      ,[Last_Name]
      ,[Universal_ID]
      ,[Employee_ID]
      ,[NPI]
      ,[State_License_Number]
      ,[Echo_DR_ID]
      ,[Business_Phone_Number]
      ,[Business_Phone_Extension]
      ,[Business_Mobile_Phone_Number]
      ,[Personal_Beeper_Pager]
      ,[Business_Email_Address]
      ,[Job_Title]
      ,[Manager_Name]
      ,[Business_Unit]
      ,[Employee_Role_Status]
      ,[Employee_Role_Status_Description]
      ,[Department_Name]
  FROM [MDM].[Active_Directory_Users_VW]
   WHERE NOT (ISNULL(Employee_ID,'99999')  LIKE '%dup%' AND Employee_Role_Status = 'Inactive')