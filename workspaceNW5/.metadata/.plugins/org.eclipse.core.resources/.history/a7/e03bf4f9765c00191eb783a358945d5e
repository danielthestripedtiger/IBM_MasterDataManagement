SELECT TO_CHAR(lg.cont_id), 'D-XNWPartyAddressBObjExt' AS OBJECT_TYPE, CDADDR.name AS addr_usage_tp, a.addr_line_one, 
a.addr_line_two, a.city_name,STATETP.NAME AS prov_state_tp,a.postal_code,
COUNTRYTP.NAME AS country_tp, TO_CHAR(a.latitude_degrees), TO_CHAR(a.longitude_degrees), 
SRC.NAME AS SOURCE_IDENT_TP, TO_CHAR(lg.start_dt), 
TO_CHAR(lg.end_dt), TO_CHAR(lg.last_verified_dt), ag.x_credential_status, 
ag.x_deactivation_flag,ag.x_fax, ag.x_phone, ag.x_phone_ext, 
ag.x_primary_billing_loc_flag, ag.x_primary_office_flag, ag.x_provider_facility_staff_code,ag.x_provider_status, 
ag.x_schd_prim_flg, ag.x_sec_phone_ext, ag.x_secondary_phone, TO_CHAR(a.x_activation_date), 
a.x_county,TO_CHAR(a.x_deactivation_date), a.x_deactivation_flag, a.x_department_code, 
a.x_department_description, a.x_facility_code,a.x_facility_name, a.x_fax, 
a.x_hospital, a.x_location_code, a.x_location_description, a.x_location_id, 
a.x_location_mnemonic,a.x_location_name, a.x_newport_key,TO_CHAR(a.x_newport_key_lastverifieddate), 
a.x_newport_key_source, a.x_phone,a.x_phone_ext,a.x_secondary_phone,
a.x_sec_phone_ext, a.x_tax_id_number, TO_CHAR(a.x_tax_id_lastverifieddate), a.x_tax_id_number_source 
FROM ADDRESSGROUP AG
LEFT JOIN LOCATIONGROUP LG
ON AG.LOCATION_GROUP_ID = LG.LOCATION_GROUP_ID
LEFT JOIN ADDRESS A
ON ag.address_id = A.address_id
LEFT JOIN CDADDRUSAGETP CDADDR 
ON AG.addr_usage_tp_cd = CDADDR.addr_usage_tp_cd
LEFT JOIN CDCOUNTRYTP COUNTRYTP 
ON a.country_tp_cd = countrytp.country_tp_cd
LEFT JOIN CDPROVSTATETP STATETP 
ON a.prov_state_tp_cd = STATETP.prov_state_tp_cd
LEFT JOIN CDSOURCEIDENTTP SRC 
ON LG.SOURCE_IDENT_TP_CD = src.source_ident_tp_cd


UNION ALL


