 	WHERE G.OBJECT_NAME = 'com.dwl.tcrm.coreParty.component.TCRMContactMethodBObj';
 	
INSERT INTO MDMDUSER.GROUPDWLTABLE (GROUP_DWLTABLE_ID, APPLICATION, GROUP_NAME, DWLTABLE_TP_CD, LAST_UPDATE_DT)
	SELECT MDMDUSER.GROUPDWLTABLE_ID_SEQ.nextval, 'TCRM', 'XNWContactMethodBObjExt', T.DWLTABLE_TP_CD, CURRENT_TIMESTAMP
	FROM MDMDUSER.GROUPDWLTABLE T
	JOIN MDMDUSER.V_GROUP G
		ON T.GROUP_NAME = G.GROUP_NAME
	WHERE G.OBJECT_NAME = 'com.dwl.tcrm.coreParty.component.TCRMContactMethodBObj';

